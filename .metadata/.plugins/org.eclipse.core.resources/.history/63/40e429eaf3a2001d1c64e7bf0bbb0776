
public class Practise {

	public static void main(String[] args) {
		
		
	        String a = "@ab$cd";
	        char[] b = a.toCharArray();
	        int c = b.length;
	        for (int i = 0; i < c / 2; i++) {
	            if (Character.isAlphabetic(b[i]) || Character.isDigit(b[i])) {
	                char temp = b[i];
	                b[i] = b[c - i - 1];
	                b[c - i - 1] = temp;
	            }
	        }
	    System.out.println(String.valueOf(b));
	

	}
}
